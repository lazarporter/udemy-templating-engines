<%- include('./includes/header.ejs') %>
    <h1>Users</h1>
    <% if (users.length > 0) { %>
        <ul>
            <% for (let user of users) { %>
                <li>
                    <p><%= user.name %></p>
                </li>
            <% } %>
        </ul>
    <% } else { %>
        <p>No users found!</p>
    <% } %>
<%- include('./includes/close.ejs') %>